  "collaboration_framework": {
    "decision_hierarchy": {
      "withdrawal_sequencing": {
        "primary_authority_domain": "tax_optimization",
        "required_consultation": ["portfolio_management"],
        "can_veto": ["estate_planning"],
        "rationale": "Tax efficiency drives sequencing, but investment sustainability and estate protection must align"
      },
      "estate_structure": {
        "primary_authority_domain": "estate_planning",
        "required_consultation": ["tax_optimization"],
        "can_veto": ["portfolio_management"],
        "rationale": "Legal structure is primary, but tax efficiency and portfolio impact must be considered"
      },
      "healthcare_coverage": {
        "primary_authority_domain": "healthcare_planning",
        "required_consultation": ["tax_optimization", "retirement_benefits"],
        "rationale": "Coverage timing and cost are primary; IRMAA thresholds create tax coordination points"
      },
      "benefits_claiming": {
        "primary_authority_domain": "retirement_benefits",
        "required_consultation": ["portfolio_management", "tax_optimization"],
        "rationale": "Claiming strategy impacts household cash flow and tax brackets"
      },
      "insurance_needs": {
        "primary_authority_domain": "risk_management",
        "required_consultation": ["estate_planning"],
        "rationale": "Risk assessment is primary; estate protection considerations may adjust coverage needs"
      }
    },
    "collaboration_patterns": [
      {
        "members": ["tax_optimization", "portfolio_management"],
        "focus": "withdrawal sequencing and tax-efficient rebalancing"
      },
      {
        "members": ["healthcare_planning", "retirement_benefits"],
        "focus": "Medicare enrollment timing with Social Security claiming"
      },
      {
        "members": ["estate_planning", "risk_management"],
        "focus": "life insurance needs and asset protection strategies"
      }
    ]
